
{   "manifest_version": 2,
    "name": "Custom Background Changer",
    "version": "1.0",
    "description": "Adds a custom background to Google.com",
    
    "page_action": {
      "default_icon": {
        "16": "images/star16.png",
        "32": "images/star32.png",
        "48": "images/star48.png",
        "128": "images/star128.png"
      }
    },
    "icons": {
        "16": "images/star16.png",
        "32": "images/star32.png",
        "48": "images/star48.png",
        "128": "images/star128.png"
  },
  "content_scripts":[{
      "matches": ["https://www.google.com/*"],
    "css":["main.css"]
  }
]
}

